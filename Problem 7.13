#include <stdio.h>
int main(){
    int i,j,row;
    scanf("%d",&row);
    for(i=0;i<=row;i++)
    {
        for(j=0;j<=row;j++)
        {
          if(i==0 || i==row)
          {
              printf("* ");
          }
          else if(j==0 || j==row)
          {
              printf("* ");
          }
          else
          {
              printf("  ");
          }
        }
        printf("\n");

    }
}
