#include <stdio.h>
int main(){
    int a,b;
    char ch;
    scanf("%c",&ch);
    switch(ch)
    {
        case '+':
        scanf("%d %d",&a,&b);
        printf("%d",a+b);
        break;
        
        case '-':
        scanf("%d %d",&a,&b);
        printf("%d",a-b);
        break;
        
        case '/':
        scanf("%d %d",&a,&b);
        if(a/0 || b/0)
        {
            printf("Invalid Calculation");
        }
        else
        {
            printf("%d",a/b);
        }
        break;
        
        case '*':
        scanf("%d %d",&a,&b);
        printf("%d",a*b);
        break;
    }
}
